<Window x:Class="Lab1_4_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1_4_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="X_TextBox" HorizontalAlignment="Left" Margin="311,50,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="134" Height="24"/>
        <TextBox x:Name="Y_TextBox" HorizontalAlignment="Left" Margin="311,100,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="134" Height="24"/>
        <TextBox x:Name="Result_TextBox" HorizontalAlignment="Left" Margin="311,150,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="134" Height="24"/>
        <Button x:Name="Result_Button" Content="Button" HorizontalAlignment="Left" Margin="311,200,0,0" VerticalAlignment="Top" Width="133" Click="Result_Button_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="274,50,0,0" TextWrapping="Wrap" Text="X:" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="274,100,0,0" TextWrapping="Wrap" Text="Y:" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="250,150,0,0" TextWrapping="Wrap" Text="Result:" VerticalAlignment="Top"/>
    </Grid>
</Window>
